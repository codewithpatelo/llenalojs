{
  "name": "llenaloJs",
  "version": "1.0.0",
  "description": "Llena formularios de un manera sencilla :)",
  "main": "js/index.js",
  "scripts": {
    "test": "nyc mocha",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint-fix": "eslint --ignore-path .eslintignore . -- --fix",
    "cover": "nyc mocha && nyc report --reporter=text-lcov | coveralls",
    "report-cover": "nyc report --reporter=text-lcov | coveralls",
    "clean": "nyc mocha && npm run lint -- --fix",
    "vul-check": "npm audit",
    "zip": "node js/zip.js",
    "encrypt": "node js/encrypt.js",
    "decrypt": "node js/decrypt.js"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patelotech/llenalojs.git"
  },
  "keywords": [
    "chrome",
    "automatizacion",
    "formularios"
  ],
  "author": "Patricio J. Gerpe",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "dotenv": "^6.2.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-chai": "0.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0",
    "simple-encryptor": "^2.0.0",
    "zip-a-folder": "0.0.7"
  },
  "dependencies": {}
}
